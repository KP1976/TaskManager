{
  "compilerOptions": {
    "newLine": "LF",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "es6",
    "downlevelIteration": true,
    "lib": ["es6"],
    "outDir": "dist",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true
  }
}